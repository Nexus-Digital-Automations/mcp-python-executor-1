{
  "name": "mcp-python-executor",
  "version": "0.3.1",
  "description": "MCP server for executing Python code with advanced monitoring and safety features",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "start": "node build/index.js",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "python",
    "executor",
    "code-execution",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "python-shell": "^5.0.0",
    "upgrade": "^1.1.0",
    "cross-spawn": "^7.0.6",
    "axios": "^1.6.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bsmi021/python-executor"
  },
  "bugs": {
    "url": "https://github.com/bsmi021/python-executor"
  }
}